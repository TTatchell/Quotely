<h1>Log In</h1>
<%= form_with url: sign_in_path do |f| %>
  <p>
    <%= f.label 'Email'%><br>
    <%= f.text_field :email, id: 'email' %>
  </p>
  <p>
    <%= f.label 'Password'%><br>
    <%= f.password_field :password, id: 'password' %><br>
    <%= link_to 'Forgot your password?', password_reset_path %>
  </p>
  <p>
    <%= f.submit 'Log In' %>
  </p>
<% end %>
